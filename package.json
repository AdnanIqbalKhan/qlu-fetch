{
  "name": "@adnaniqbal/fetch",
  "version": "1.0.4",
  "description": "A fetch wrapper with retries",
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/cjs/types/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm pack --pack-destination dist",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv dist/esm/index.js dist/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json && mv dist/cjs/index.js dist/cjs/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/adnaniqbalkhan/qlu-fetch.git"
  },
  "keywords": [
    "fetch",
    "node-fetch",
    "retry",
    "api"
  ],
  "author": "adnaniqbal",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/adnaniqbalkhan/qlu-fetch/issues"
  },
  "homepage": "https://gitlab.com/adnaniqbalkhan/qlu-fetch#readme",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "node-fetch": "^3.3.0"
  }
}